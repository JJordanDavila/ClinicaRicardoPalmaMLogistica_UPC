
@{
    ViewBag.Title = "Validar";
}

<h2>Validar</h2>

<form id="frmNuevoParametro">
        
        <div class="form-group colFormularioHorizontal">
            @Html.LabelFor(model => model.RUC, new { @class = "col-sm-2 control-label obligatorio" })
            <div class="col-md-3">
                @Html.EditorFor(model => model.RUC, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.RUC, "", new { @class = "text-danger" })
            </div>
            <div class="col-sm-12"></div>
        </div>
        <div class="form-group colFormularioHorizontal">
            @Html.LabelFor(model => model.RazonSocial, new { @class = "col-sm-2 control-label obligatorio" })
            <div class="col-md-3">
                @Html.EditorFor(model => model.RazonSocial, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.RazonSocial, "", new { @class = "text-danger" })
            </div>
            <div class="col-sm-12"></div>
        </div>
    <div class="form-group colFormularioHorizontal">
        @Html.LabelFor(model => model.Correo, new { @class = "col-sm-2 control-label obligatorio" })
        <div class="col-md-3">
            @Html.EditorFor(model => model.Correo, new { htmlAttributes = new { @class = "form-control" } })
            @Html.ValidationMessageFor(model => model.Correo, "", new { @class = "text-danger" })
        </div>
        <div class="col-sm-12"></div>
    </div>

        <div class="form-group colFormularioHorizontal">
            @Html.LabelFor(model => model.Rubro,  new { @class = "col-sm-2 control-label obligatorio" })
            <div class="col-md-5">
                @Html.EditorFor(model => model.Rubro, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.Rubro, "", new { @class = "text-danger" })
            </div>
            <div class="col-sm-12"></div>
        </div>

         
    <div class="modal-footer" style="margin-top: 20px;">
        <a href="@Url.Action("Index", "Parametro")" class="btn btn-primary" style="color: white;"><span class="glyphicon glyphicon-ok-sign" id="btnCancelar"></span> Cancelar</a>
        <button type="button" class="btn btn-primary" id="btnGuardar">Guardar</button>
    </div>
</form>
